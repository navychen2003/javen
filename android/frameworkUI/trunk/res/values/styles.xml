<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ProgressBar">
    </style>
    
    <style name="ProgressBar.Small" parent="@*android:style/Widget.Holo.Light.ProgressBar">
        <item name="android:minWidth">24dp</item>
        <item name="android:maxWidth">24dp</item>
        <item name="android:minHeight">24dp</item>
        <item name="android:maxHeight">24dp</item>
    </style>
    
    <style name="ProgressBar.Medium" parent="@*android:style/Widget.Holo.Light.ProgressBar">
    </style>
    
    <style name="ProgressBar.Large" parent="@*android:style/Widget.Holo.Light.ProgressBar.Large">
    </style>
    
    <style name="ActionButton" parent="@*android:style/Widget.Holo.Light.ActionButton">
    </style>
    
    <style name="ActionBar.Title" parent="TextAppearance.Sherlock.Widget.ActionBar.Title">
        <item name="android:textSize">@dimen/action_bar_title_text_size</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:lineSpacingMultiplier">0.8</item>
    </style>
    
    <style name="ActionBar.Subtitle" parent="TextAppearance.Sherlock.Widget.ActionBar.Subtitle">
        <item name="android:textSize">@dimen/action_bar_subtitle_text_size</item>
        <item name="android:textColor">#ff888888</item>
        <item name="android:lineSpacingMultiplier">0.8</item>
